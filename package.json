{
  "name": "dulcet-uglifyjs-webpack-plugin",
  "version": "0.4.6",
  "description": "UglifyJS plugin for webpack",
  "main": "./dist",
  "scripts": {
    "build": "babel src -d dist",
    "test:all": "npm run test:coverage && npm run test:lint",
    "test": "jest --",
    "test:coverage": "jest --coverage --",
    "test:watch": "jest --watch --",
    "test:lint": "eslint . --ext .js --ignore-path .gitignore --cache",
    "preversion": "npm run test:all && npm run build && git commit --allow-empty -am \"Update dist\"",
    "postinstall": "node lib/post_install.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/khanhduy1407/dulcet-uglifyjs-webpack-plugin.git"
  },
  "keywords": [
    "webpack",
    "uglifyjs",
    "plugin"
  ],
  "files": [
    "dist",
    "lib"
  ],
  "author": "NKDuy",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/khanhduy1407/dulcet-uglifyjs-webpack-plugin/issues"
  },
  "homepage": "https://github.com/khanhduy1407/dulcet-uglifyjs-webpack-plugin",
  "peerDependencies": {
    "@dulcetjs/webpack": "^1.9 || ^2 || ^2.1.0-beta || ^2.2.0-rc || ^3.0.0"
  },
  "dependencies": {
    "source-map": "^0.5.6",
    "dulcet-uglify-js": "^2.8.29",
    "webpack-sources": "^1.0.1"
  },
  "devDependencies": {
    "@dulcetjs/webpack": "^2.2.2",
		"@nkduy/babel-cli": "^6.22.0",
		"@nkduy/babel-core": "^6.24.2",
		"@nkduy/babel-plugin-syntax-object-rest-spread": "^6.13.0",
		"@nkduy/babel-plugin-transform-object-rest-spread": "^6.22.0",
		"@nkduy/babel-preset-es2015": "^6.22.0",
		"babel-eslint": "^7.1.1",
		"eslint": "^3.13.1",
		"eslint-config-airbnb": "^14.0.0",
		"eslint-plugin-dulcet": "^7.1.0",
		"eslint-plugin-import": "^2.2.0",
		"eslint-plugin-jsx-a11y": "^3.0.2",
		"eslint-plugin-node": "^4.0.1",
		"git-prepush-hook": "^1.0.1",
		"sync-exec": "^0.6.2"
	},
  "engines": {
    "node": ">=4.3.0 <5.0.0 || >=5.10"
  },
  "pre-push": [
    "test:all"
  ]
}
